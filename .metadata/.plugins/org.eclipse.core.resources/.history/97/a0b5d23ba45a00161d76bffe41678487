/*
 *자바의 GUI관련 API들은 JAVA.AWT패키지에 지원,
 *윈도우와 같은 컴포넌트들을 포함하는 객체는 배치 - 5가지의 배치관리자 실습. */

package gui;

import java.awt.BorderLayout;
import java.awt.Button;
import java.awt.Frame;

public class BorderTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			Frame frame = new Frame();
			Button btn_north = new Button();
			Button btn_west = new Button();
			Button btn_center = new Button();
			Button btn_east = new Button();
			Button btn_south = new Button();
			
			
			//borderLayout 적용
			BorderLayout b_layout = new BorderLayout();
			
			frame.setLayout(b_layout);
			
			
	}

}

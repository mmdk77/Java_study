package com.sds.gui;

import javax.swing.JFrame;
import javax.swing.JOptionPane;
import javax.swing.JPanel;

public class TablePrac  extends JFrame{
	JPanel p_west,p_east;
	JOptionPane op;
	
	public TablePrac() {
		// TODO Auto-generated constructor stub
		super("테이블실습");
		
		p_west = new JPanel();
		p_east = new JPanel();
		op = new JOptionPane();
		
		
		p_west.add(op);
		
		add(p_west	);
		
		
		setSize(700,600);
		setVisible(true);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		
	}
	
	public static void main(String[] args){
		new TablePrac();
	}
}

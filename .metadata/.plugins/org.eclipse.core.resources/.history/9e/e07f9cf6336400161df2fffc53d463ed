public class GameWin extends JFrame{	
	GamePanel game_p;
	
	JMenuBar bar;
	JMenu[] menu = new JMenu[2];
	JMenuItem[] item = new JMenuItem[3]; 
	public GameWin() {
		// TODO Auto-generated constructor stub
		
		
		
	
		add(game_p= new GamePanel());
		pack();

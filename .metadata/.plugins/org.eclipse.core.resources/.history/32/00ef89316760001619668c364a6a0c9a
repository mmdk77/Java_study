package com.sds.auto;

import java.awt.Dimension;

import javax.swing.JProgressBar;

public class ProgressBar extends JProgressBar {
	
	Thread thread;
	int n;
	
	public ProgressBar() {
		// TODO Auto-generated constructor stub

		setPreferredSize(new Dimension(600, 50));
		fill();
	}
	public void fill() {
		n++;
		this.setValue(n);
	}

}

package com.sds.auto;

import java.awt.Dimension;

import javax.swing.JProgressBar;

public class ProgressBar extends JProgressBar {
	
	JProgressBar bar;
	
	public ProgressBar() {
		// TODO Auto-generated constructor stub
		public void fill() {
			n++;
			bar1.setValue(n);
		}
		setPreferredSize(new Dimension(300, 50));
	}

}
